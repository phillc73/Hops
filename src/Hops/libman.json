{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.2.1",
      "destination": "wwwroot/lib/twitter-bootstrap/",
      "files": [
        "css/bootstrap.min.css",
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.bundle.min.js.map",
        "css/bootstrap.min.css.map"
      ]
    },
    {
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js",
        "jquery.min.map"
      ]
    },
    {
      "library": "typeahead.js@0.11.1",
      "destination": "wwwroot/lib/typeahead.js/",
      "files": [
        "typeahead.bundle.min.js"
      ]
    },
    {
      "library": "sortable@0.8.0",
      "destination": "wwwroot/lib/sortable/",
      "files": [
        "js/sortable.min.js",
        "css/sortable-theme-bootstrap.min.css"
      ]
    },
    {
      "library": "mark.js@8.11.1",
      "destination": "wwwroot/lib/mark.js/",
      "files": [
        "jquery.mark.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "brauhaus@1.1.1",
      "destination": "wwwroot/lib/brauhaus/",
      "files": [
        "lib/brauhaus.min.js"
      ]
    }
  ]
}